{
  "name": "electron_remote_demo",
  "productName": "ElectronRemoteDemo",
  "version": "0.0.1",
  "description": "Electron Remote Bug Demo",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [
  ],
  "license": "UNLICENSED",
  "config": {
    "forge": {
      "packagerConfig": {
        "copyright": "Copyright Â© 2021 QiYuTech",
        "extraResource": [
          "app"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "seo"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                },
                {
                  "html": "./src/front/pages/settings/settings_index.html",
                  "js": "./src/front/pages/settings/settings_index.tsx",
                  "name": "settings_index"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "6.0.0-beta.57",
    "@types/lodash": "^4.14.168",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@vercel/webpack-asset-relocator-loader": "^1.4.0",
    "bulma": "^0.9.2",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.0",
    "electron": "^13.1.0",
    "electron-build-env": "^0.2.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "node-loader": "^2.0.0",
    "sass": "^1.34.1",
    "sass-loader": "^12.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.2",
    "webpack-node-externals": "^3.0.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@electron/remote": "^1.1.0",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^5.0.0-alpha.30",
    "@material-ui/icons": "^5.0.0-alpha.28",
    "@material-ui/lab": "^5.0.0-alpha.30",
    "electron-log": "^4.3.2",
    "electron-settings": "^4.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "node-polyfill-webpack-plugin": "^1.1.2",
    "node-util": "^0.0.1",
    "path-browserify": "^1.0.1",
    "pretty-error": "^3.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
